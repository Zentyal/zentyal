    background-color: transparent;
    width: 200px;
}
.legendLabel {
    font-size: 12px;
}
.legendColorBox > div {
    border: medium none !important;
    height: 20px;
    position: relative;
    width: 20px;
}
.legendColorBox > div > div {
    border-width: 10px !important;
    height: 0 !important;
    position: absolute;
    width: 0 !important;
}

.pieLabel > div {
