<%args>
@items
$current => 'none'
$currentUrl => ''
</%args>
<div id='menu'>
    <ul id='nav'>
        <li class="menuSearch">
            <input id='menusearch' type='text'/>
        </li>
% foreach my $item (@items) {
        <% $item->html($current, $currentUrl) %>
% }
</ul></div>
<script type="text/javascript">
$('#menusearch').keyup(Zentyal.MenuSearch.filterMenu);
</script>
